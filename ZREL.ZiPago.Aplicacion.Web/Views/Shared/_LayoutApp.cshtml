<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>@ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <meta name="msapplication-TileImage" content="https://www.zipago.pe/wp-content/uploads/2019/07/logo_favicon.png">
    <link rel="icon" href="~/wp-content/uploads/2019/07/logo_favicon-142x150.png" sizes="32x32">
    <link rel="icon" href="~/wp-content/uploads/2019/07/logo_favicon.png" sizes="192x192">
    <link rel="apple-touch-icon-precomposed" href="~/wp-content/uploads/2019/07/logo_favicon.png">

    <link rel="stylesheet" href="https://netdna.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.css">
    @*<link rel="stylesheet" href="~/dist/bootstrap-4.3.1-dist/css/bootstrap.min.css">*@

    <link rel="stylesheet" href="~/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/gsdk-bootstrap-wizard.css"/>
    <link rel="stylesheet" href="~/wp-includes/css/site.css">

</head>
<body style="background-color:#FF6900;">

    <nav class="navbar navbar-default navbar-fixed-top">        
        <div class="row">
            <div class="col-sm-4"></div>
            <div class="col-sm-2 navbar-text text-right" style="padding-right:0px; margin-right:0px; margin-bottom: 0px;">
                <img alt="ZiPago" src="~/wp-content/uploads/2019/07/Imagotipo_ZiPago-118x75.png">                
            </div>
            <div class="col-sm-2 navbar-text text-left" style="padding-left:0px; margin-top: 8px; margin-left:0px; margin-bottom: 0px; ">                
                <h4 style="color:black">
                    <strong>&nbsp;@ViewData["TitleNav"]</strong>
                </h4>                
            </div>
            <div class="col-sm-4"></div>
        </div>        
    </nav>

    @RenderBody()

    @*<script type='text/javascript' src='~/dist/bootstrap-4.3.1-dist/js/bootstrap.min.js'></script>*@
        
    <script src="~/js/jquery-2.2.4.min.js" type="text/javascript"></script>
    <script src="~/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/js/jquery.bootstrap.wizard.js" type="text/javascript"></script>    
    <script src="~/js/gsdk-bootstrap-wizard.js"></script>    
    <script src="~/js/jquery.validate.min.js"></script>

</body>

</html>